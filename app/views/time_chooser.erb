<body>
    <div class="container">

        <div class="row">
            <div class="box">
                <div class="col-lg-12">
                    <hr>
                    <h2 class="intro-text text-center">
                        <strong>Choose session time</strong>
                    </h2>
                    <hr>
                </div>
                <div class="col-md-6">
               
                </div>
                <div class="col-md-6">
                   
                </div>
                <div class="clearfix"></div>
            </div>
        </div>

        <div class="row">
            <div class="box">
                <div class="col-lg-12">
                    <hr>
                    <table>
                      <p>Pick a time on <%= @selected_date %> from 7:00am to 3:00pm</p>
                      <div id="calendar"></div>
                      <% (7..15).each do |num| %>
                      <tr>
                        <td>
                        <%= num %>:00<br>
                        </td>
                        <td></td>
                      </tr>
                      <tr>
                        <td>
                        <%= num %>:15<br>
                        </td>
                        <td></td>
                      </tr>
                      <tr>
                        <td>
                        <%= num %>:30<br>
                        </td>
                        <td></td>
                      </tr>
                      <tr>
                        <td>
                        <%= num %>:45<br>
                        </td>
                        <td></td>
                      </tr>
                      <% end %>
                    </table>
                    <!-- lol -->
                    <button ><a href="https://www.youtube.com/watch?v=dQw4w9WgXcQ">Submit</a></button>
                    
                      

                </div>
                <div class="clearfix"></div>
            </div>
        </div>

    </div>
    <!-- /.container -->

    <footer>
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <p>Copyright &copy; Okanagan Swag</p>
                </div>
            </div>
        </div>
    </footer>

    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

</body>